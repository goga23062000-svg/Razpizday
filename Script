-- –ß–∞—Å—Ç—å 1: Anti-Tsunami Cheat –¥–ª—è Escape the Tsunami
-- –ó–∞—â–∏—â–∞–µ—Ç –æ—Ç —Å–º–µ—Ä—Ç–µ–ª—å–Ω–æ–π –≤–æ–ª–Ω—ã –∏ –¥–∞–µ—Ç –±–µ—Å—Å–º–µ—Ä—Ç–∏–µ

-- Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")

-- –ò–≥—Ä–æ–∫
local player = Players.LocalPlayer

-- –ü–µ—Ä–µ–º–µ–Ω–Ω—ã–µ
local isImmortal = false
local originalHealth = 100
local isScriptActive = true

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∏—Å–∫–∞ –≤–æ–ª–Ω—ã-—É–±–∏–π—Ü—ã
local function findKillerWave()
    for _, part in ipairs(Workspace:GetDescendants()) do
        -- –ò—â–µ–º —á–∞—Å—Ç–∏ —Å –ø–æ–¥–æ–∑—Ä–∏—Ç–µ–ª—å–Ω—ã–º–∏ –∏–º–µ–Ω–∞–º–∏ (–≤–æ–ª–Ω–∞, —Ü—É–Ω–∞–º–∏, —É–±–∏–π—Ü–∞ –∏ —Ç.–¥.)
        if part:IsA("BasePart") then
            local nameLower = part.Name:lower()
            if nameLower:find("wave") or 
               nameLower:find("tsunami") or 
               nameLower:find("water") or
               nameLower:find("kill") or
               nameLower:find("death") or
               nameLower:find("damage") then
                
                -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –µ—Å—Ç—å –ª–∏ —É —á–∞—Å—Ç–∏ —Å–∫—Ä–∏–ø—Ç—ã –∏–ª–∏ touch –∏–Ω—Ç–µ—Ä–µ—Å—Ç
                for _, child in ipairs(part:GetDescendants()) do
                    if child:IsA("Script") or child:IsA("LocalScript") then
                        return part, child
                    end
                end
                
                -- –ü—Ä–æ–≤–µ—Ä—è–µ–º Touch –∏–Ω—Ç–µ—Ä–µ—Å—Ç
                if part:FindFirstChildWhichIsA("TouchInterest") then
                    return part, part:FindFirstChildWhichIsA("TouchInterest")
                end
            end
        end
    end
    return nil, nil
end

-- –§—É–Ω–∫—Ü–∏—è –∑–∞—â–∏—Ç—ã –æ—Ç –≤–æ–ª–Ω—ã
local function protectFromWave()
    while isScriptActive do
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            
            if humanoid then
                -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω–æ–µ –∑–¥–æ—Ä–æ–≤—å–µ
                if humanoid.Health > 0 then
                    originalHealth = humanoid.Health
                end
                
                -- –ê–∫—Ç–∏–≤–∏—Ä—É–µ–º –±–µ—Å—Å–º–µ—Ä—Ç–∏–µ
                if isImmortal then
                    -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –∑–¥–æ—Ä–æ–≤—å–µ, –µ—Å–ª–∏ –æ–Ω–æ —É–ø–∞–ª–æ
                    if humanoid.Health <= 10 then
                        humanoid.Health = originalHealth
                    end
                    
                    -- –ó–∞—â–∏—Ç–∞ –æ—Ç —Å–º–µ—Ä—Ç–∏
                    humanoid:SetStateEnabled(Enum.HumanoidStateType.Dead, false)
                    
                    -- –û—Ç–∫–ª—é—á–∞–µ–º —É—Ä–æ–Ω
                    for _, connection in ipairs(getconnections(humanoid.Died)) do
                        connection:Disable()
                    end
                end
            end
            
            -- –ò—â–µ–º –∏ –Ω–µ–π—Ç—Ä–∞–ª–∏–∑—É–µ–º –≤–æ–ª–Ω—É-—É–±–∏–π—Ü—É
            local wavePart, waveScript = findKillerWave()
            if wavePart then
                -- –ú–µ—Ç–æ–¥ 1: –î–µ–ª–∞–µ–º –≤–æ–ª–Ω—É –±–µ–∑–≤—Ä–µ–¥–Ω–æ–π
                wavePart.CanTouch = false
                wavePart.CanCollide = false
                
                -- –ú–µ—Ç–æ–¥ 2: –û—Ç–∫–ª—é—á–∞–µ–º —Å–∫—Ä–∏–ø—Ç—ã
                if waveScript then
                    waveScript.Disabled = true
                end
                
                -- –ú–µ—Ç–æ–¥ 3: –£–¥–∞–ª—è–µ–º Touch –∏–Ω—Ç–µ—Ä–µ—Å—Ç
                local touchInterest = wavePart:FindFirstChildWhichIsA("TouchInterest")
                if touchInterest then
                    touchInterest:Destroy()
                end
                
                -- –ú–µ—Ç–æ–¥ 4: –î–µ–ª–∞–µ–º –Ω–µ–≤–∏–¥–∏–º–æ–π (–æ–ø—Ü–∏–æ–Ω–∞–ª—å–Ω–æ)
                wavePart.Transparency = 1
                wavePart.LocalTransparencyModifier = 1
            end
        end
        
        task.wait(0.1) -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –∫–∞–∂–¥—ã–µ 0.1 —Å–µ–∫—É–Ω–¥—ã
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è/–≤—ã–∫–ª—é—á–µ–Ω–∏—è –±–µ—Å—Å–º–µ—Ä—Ç–∏—è
local function toggleImmortality()
    isImmortal = not isImmortal
    
    local character = player.Character
    if character then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            if isImmortal then
                -- –í–∫–ª—é—á–∞–µ–º –±–µ—Å—Å–º–µ—Ä—Ç–∏–µ
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Dead, false)
                humanoid.Health = 100
                print("üõ°Ô∏è –ë–µ—Å—Å–º–µ—Ä—Ç–∏–µ –í–ö–õ–Æ–ß–ï–ù–û")
            else
                -- –í—ã–∫–ª—é—á–∞–µ–º –±–µ—Å—Å–º–µ—Ä—Ç–∏–µ
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Dead, true)
                print("üõ°Ô∏è –ë–µ—Å—Å–º–µ—Ä—Ç–∏–µ –í–´–ö–õ–Æ–ß–ï–ù–û")
            end
        end
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª–Ω–æ–≥–æ –æ—Ç–∫–ª—é—á–µ–Ω–∏—è –≤–æ–ª–Ω—ã
local function disableWaveCompletely()
    local wavePart, waveScript = findKillerWave()
    if wavePart then
        -- –ü–æ–ª–Ω–æ—Å—Ç—å—é —É–¥–∞–ª—è–µ–º –≤–æ–ª–Ω—É
        wavePart:Destroy()
        print("üåä –í–æ–ª–Ω–∞ –ø–æ–ª–Ω–æ—Å—Ç—å—é —É–¥–∞–ª–µ–Ω–∞!")
        return true
    end
    return false
end

-- –°–æ–∑–¥–∞–µ–º –ø—Ä–æ—Å—Ç–æ–π GUI –¥–ª—è —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è
local function createAntiWaveGUI()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "AntiTsunamiGUI"
    screenGui.Parent = player:WaitForChild("PlayerGui")
    
    -- –û—Å–Ω–æ–≤–Ω–∞—è –∫–Ω–æ–ø–∫–∞
    local mainButton = Instance.new("TextButton")
    mainButton.Name = "MainButton"
    mainButton.Size = UDim2.new(0, 50, 0, 50)
    mainButton.Position = UDim2.new(1, -60, 0.5, -25)
    mainButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
    mainButton.Text = "üõ°Ô∏è"
    mainButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    mainButton.Font = Enum.Font.GothamBold
    mainButton.TextSize = 24
    mainButton.AutoButtonColor = false
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 10)
    corner.Parent = mainButton
    
    mainButton.Parent = screenGui
    
    -- –ú–µ–Ω—é —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è
    local menuFrame = Instance.new("Frame")
    menuFrame.Name = "MenuFrame"
    menuFrame.Size = UDim2.new(0, 200, 0, 150)
    menuFrame.Position = UDim2.new(1, -210, 0.5, -75)
    menuFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    menuFrame.BackgroundTransparency = 0.1
    menuFrame.Visible = false
    
    local menuCorner = Instance.new("UICorner")
    menuCorner.CornerRadius = UDim.new(0, 10)
    menuCorner.Parent = menuFrame
    
    menuFrame.Parent = screenGui
    
    -- –ö–Ω–æ–ø–∫–∏ –º–µ–Ω—é
    local buttons = {
        {
            text = "–ë–µ—Å—Å–º–µ—Ä—Ç–∏–µ: –í–´–ö–õ",
            name = "ImmortalityToggle",
            color = Color3.fromRGB(100, 100, 255)
        },
        {
            text = "–£–¥–∞–ª–∏—Ç—å –≤–æ–ª–Ω—É",
            name = "DeleteWave",
            color = Color3.fromRGB(255, 100, 100)
        },
        {
            text = "–í–æ—Å—Å—Ç–∞–Ω–æ–≤–∏—Ç—å HP",
            name = "Heal",
            color = Color3.fromRGB(100, 255, 100)
        }
    }
    
    local buttonHeight = 40
    for i, btnInfo in ipairs(buttons) do
        local button = Instance.new("TextButton")
        button.Name = btnInfo.name
        button.Size = UDim2.new(1, -20, 0, buttonHeight)
        button.Position = UDim2.new(0, 10, 0, 10 + (i-1)*(buttonHeight + 10))
        button.BackgroundColor3 = btnInfo.color
        button.Text = btnInfo.text
        button.TextColor3 = Color3.fromRGB(255, 255, 255)
        button.Font = Enum.Font.Gotham
        button.TextSize = 14
        button.AutoButtonColor = false
        
        local btnCorner = Instance.new("UICorner")
        btnCorner.CornerRadius = UDim.new(0, 8)
        btnCorner.Parent = button
        
        button.Parent = menuFrame
    end
    
    -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –∫–Ω–æ–ø–æ–∫
    mainButton.MouseButton1Click:Connect(function()
        menuFrame.Visible = not menuFrame.Visible
    end)
    
    menuFrame:FindFirstChild("ImmortalityToggle").MouseButton1Click:Connect(function()
        toggleImmortality()
        local btn = menuFrame:FindFirstChild("ImmortalityToggle")
        if isImmortal then
            btn.Text = "–ë–µ—Å—Å–º–µ—Ä—Ç–∏–µ: –í–ö–õ"
            btn.BackgroundColor3 = Color3.fromRGB(50, 255, 50)
        else
            btn.Text = "–ë–µ—Å—Å–º–µ—Ä—Ç–∏–µ: –í–´–ö–õ"
            btn.BackgroundColor3 = Color3.fromRGB(100, 100, 255)
        end
    end)
    
    menuFrame:FindFirstChild("DeleteWave").MouseButton1Click:Connect(function()
        if disableWaveCompletely() then
            local btn = menuFrame:FindFirstChild("DeleteWave")
            btn.Text = "‚úÖ –í–æ–ª–Ω–∞ —É–¥–∞–ª–µ–Ω–∞!"
            task.wait(1)
            btn.Text = "–£–¥–∞–ª–∏—Ç—å –≤–æ–ª–Ω—É"
        end
    end)
    
    menuFrame:FindFirstChild("Heal").MouseButton1Click:Connect(function()
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.Health = 100
                local btn = menuFrame:FindFirstChild("Heal")
                btn.Text = "‚ù§Ô∏è HP –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω!"
                task.wait(1)
                btn.Text = "–í–æ—Å—Å—Ç–∞–Ω–æ–≤–∏—Ç—å HP"
            end
        end
    end)
    
    return screenGui
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º –∑–∞—â–∏—Ç—É
spawn(function()
    protectFromWave()
end)

-- –°–æ–∑–¥–∞–µ–º GUI
createAntiWaveGUI()

-- –ì–æ—Ä—è—á–∏–µ –∫–ª–∞–≤–∏—à–∏
local UserInputService = game:GetService("UserInputService")
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    if input.KeyCode == Enum.KeyCode.F1 then
        toggleImmortality()
        print("üõ°Ô∏è –ë–µ—Å—Å–º–µ—Ä—Ç–∏–µ –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–æ: " .. (isImmortal and "–í–ö–õ" or "–í–´–ö–õ"))
    end
    
    if input.KeyCode == Enum.KeyCode.F2 then
        disableWaveCompletely()
    end
    
    if input.KeyCode == Enum.KeyCode.F3 then
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.Health = 100
                print("‚ù§Ô∏è HP –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω –¥–æ 100")
            end
        end
    end
end)

